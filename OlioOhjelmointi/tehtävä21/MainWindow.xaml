<Window x:Class="tehtävä21.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tehtävä21"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <CheckBox x:Name="cb_kana" Content="CheckBox" HorizontalAlignment="Left" Margin="124,105,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.113,-0.363" Height="17" Width="79" Checked="CheckBox_Checked"/>
        <CheckBox x:Name="cb_Voi" Content="CheckBox" HorizontalAlignment="Left" Margin="124,127,0,0" VerticalAlignment="Top"/>
        <StackPanel x:Name="sp_panel" HorizontalAlignment="Left" Height="243" Margin="485,40,0,0" VerticalAlignment="Top" Width="179">
            <TextBox x:Name="tb_Textbox1" Height="23" TextWrapping="Wrap" Text="TextBox"/>
            <TextBox x:Name="tb_Textbox2" Height="23" TextWrapping="Wrap" Text="TextBox"/>
        </StackPanel>
        <Button x:Name="btn_Lisää" HorizontalAlignment="Left" Margin="298,122,0,0" VerticalAlignment="Top" Width="76" Click="Button_Click" Height="20">
            <Canvas Height="100" Width="100"/>
        </Button>
    </Grid>
</Window>
